<Defs>
  <PawnKindDef Name="Aotrs_SkeleHostWarrior">
    <defName>Aotrs_SkeleHostWarrior</defName>
    <label>roaming skeleton</label>
    <combatPower>60</combatPower>
    <race>DRSKT_Race</race>
    <defaultFactionType>Aotrs_SkeletonHorde</defaultFactionType>
    <maxGenerationAge>100</maxGenerationAge>
    <chemicalAddictionChance>0</chemicalAddictionChance>
    <invNutrition>0</invNutrition>
    <apparelTags>
      <li>IndustrialBasic</li>
      <li>IndustrialMilitaryBasic</li>
      <li>MedievalMilitary</li>
      <li>Neolithic</li>
      <li>DankPyon_Peasant</li>
      <li>DankPyon_PeasantHelmet</li>
    </apparelTags>

    <itemQuality>Poor</itemQuality>
    <gearHealthRange>0.2~1.1</gearHealthRange>
    <apparelMoney>0~600</apparelMoney>
    <weaponMoney>100~300</weaponMoney>
    <weaponTags>
      <li>MedievalMeleeDecent</li>
      <li>NeolithicMeleeBasic</li>
      <li>NeolithicMeleeDecent</li>
      <li>NeolithicMeleeAdvanced</li>
    </weaponTags>
    <techHediffsMoney>50~1500</techHediffsMoney>
    <techHediffsTags>
      <li>Poor</li>
      <li>Advanced</li>
    </techHediffsTags>
    <techHediffsChance>0.05</techHediffsChance>
    <initialWillRange>0~1</initialWillRange>
    <initialResistanceRange>40~50</initialResistanceRange>
    <factionLeader>false</factionLeader>

    <xenotypeSet Inherit="False">
      <xenotypeChances>
        <Aotrs_AniSkel>1</Aotrs_AniSkel>
      </xenotypeChances>
      <warnIfTotalAbove1>false</warnIfTotalAbove1>
    </xenotypeSet>
	
    <requiredWorkTags>
      <li>Violent</li>
    </requiredWorkTags>
    <styleItemTags>
      <li>
        <tag>Rural</tag>
        <baseWeight>1</baseWeight>
        <weightFactor>1</weightFactor>
      </li>
      <li>
        <tag>Urban</tag>
        <baseWeight>1</baseWeight>
        <weightFactor>1</weightFactor>
      </li>
      <li>
        <tag>Punk</tag>
        <baseWeight>1</baseWeight>
        <weightFactor>1</weightFactor>
      </li>
      <li>
        <tag>Tribal</tag>
        <baseWeight>1</baseWeight>
        <weightFactor>1</weightFactor>
      </li>
    </styleItemTags>
    <skills>
      <li>
        <skill>Shooting</skill>
        <range>0~5</range>
      </li>
      <li>
        <skill>Melee</skill>
        <range>0~5</range>
      </li>
    </skills>
  </PawnKindDef>


